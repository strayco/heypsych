{
  "name": "heypsych",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build:index": "node scripts/build-resource-index.cjs",
    "build": "npm run build:index && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "db:ensure": "node --env-file=.env.local scripts/setup/create-schemas.js",
    "seed:conditions": "node --env-file=.env.local scripts/seed-conditions-from-files.js",
    "seed:treatments": "node --env-file=.env.local scripts/seed-treatments-from-files.js",
    "seed:resources": "node --env-file=.env.local scripts/seed-resources-from-files.js",
    "seed:support": "node scripts/seed-support-resources.js",
    "seed:all": "npm run seed:conditions && npm run seed:treatments && npm run seed:resources && npm run seed:support",
    "seed:assessments": "node --env-file=.env.local scripts/seed-assessments-from-files.js",
    "seed:providers": "node --env-file=.env.local scripts/seed-providers.js",
    "seed": "npm run db:ensure && npm run seed:conditions && npm run seed:treatments && npm run seed:resources && npm run seed:assessments && npm run seed:providers",
    "seed-small": "node --env-file=.env.local scripts/seed-providers.js 25",
    "seed-large": "node --env-file=.env.local scripts/seed-providers.js 500",
    "import-nppes": "ts-node --transpile-only scripts/nppes-importer.ts",
    "import-nppes-sample": "tsx scripts/run-nppes-import.ts --sample=1000",
    "update-providers": "node --env-file=.env.local scripts/update-providers.js update",
    "cleanup-providers": "node --env-file=.env.local scripts/update-providers.js cleanup",
    "provider-stats": "node --env-file=.env.local scripts/update-providers.js stats",
    "provider-maintenance": "node --env-file=.env.local scripts/update-providers.js all",
    "migrate:content:dry": "tsx scripts/migrate-articles-to-knowledge-hub.ts --dry",
    "migrate:content": "tsx scripts/migrate-articles-to-knowledge-hub.ts",
    "validate:content": "tsx scripts/validate-content.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/nextjs": "^10.25.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.537.0",
    "nanoid": "^5.1.6",
    "next": "^15.5.6",
    "p-limit": "^7.1.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-use": "^17.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yargs": "^18.0.0",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/yargs": "^17.0.33",
    "csv-parser": "^3.2.0",
    "eslint": "^9",
    "eslint-config-next": "^15.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
